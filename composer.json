{
    "name": "laravel/booklist",
    "description": "book lists",
    "keywords": ["laravel", "booklist", "sql", "orm"],
    "minimum-stability": "dev",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "DemoN",
            "email": "demon@demon.com"
        }
    ],
    "config": {
        "platform": {
            "php": "7.3"
        }
    },
    "require": {
        "php": "^7.3",
        "illuminate/database": "5.8.*",
        "illuminate/events": "5.8.*",
        "illuminate/cache": "5.8.*",
        "illuminate/filesystem": "5.8.*",
        "luracast/config": "2.*",
        "vlucas/phpdotenv": "^3.3",
        "league/flysystem": "~1.0",
        "illuminate/pagination": "5.8.*",
        "restler/framework": "3.0.0-RC6",
        "bshaffer/oauth2-server-php":"^1.11",
        "monolog/monolog": "^1.24",
        "symfony/http-foundation": "^4.2",
        "kylekatarnls/update-helper": "^1.1",
        "ext-json": "*",
        "ext-pdo": "*"
    },
    "require-dev": {
        "illuminate/console": "5.8.*",
        "psy/psysh": "0.9.*",
        "symfony/process": "^4.2",
        "barryvdh/laravel-ide-helper": "^2.6@dev",
        "illuminate/queue": "^5.8",
        "illuminate/view": "^5.8",
        "doctrine/dbal": "~2.3",
        "phpunit/phpunit": "^8",
        "guzzlehttp/guzzle": "~6.0",
        "fzaninotto/faker": "^1.4"
    },
    "suggest": {
        "doctrine/dbal": "Allow renaming columns and dropping SQLite columns."
    },
    "autoload": {
        "classmap": [
            "app/commands",
            "app/models",
            "app/database/migrations",
            "app/database/seeds"
        ],
        "psr-0": {
            "": "app/controllers"
        },
        "psr-4": {
            "Bootstrap\\": "bootstrap/"
        }
    }
}
